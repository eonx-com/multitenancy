{
  "name": "loyaltycorp/multitenancy",
  "description": "Library for handling multiple tenants within a single code base",
  "keywords": [
    "loyaltycorp",
    "multitenancy"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "doctrine/orm": "^2.6.3",
    "loyaltycorp/requesthandlers": "^1.1",
    "symfony/validator": "^4.3",
    "eoneopay/utils": "^0.2.8",
    "eoneopay/framework": "dev-master"
  },
  "require-dev": {
    "eoneopay/standards": "dev-master",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "LoyaltyCorp\\Multitenancy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\LoyaltyCorp\\Multitenancy\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
