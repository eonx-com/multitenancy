{
  "name": "eonx-com/multitenancy",
  "description": "Library for handling multiple tenants within a single code base",
  "keywords": [
    "loyaltycorp",
    "multitenancy"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "doctrine/orm": "^2.6.3",
    "eonx-com/apiformats": "^1.0",
    "eonx-com/externals": "^1.0|^2.0",
    "eonx-com/flowconfig-wrapper": "^1.0.5",
    "eonx-com/framework": "^0.2",
    "eonx-com/requesthandlers": "^1.1",
    "eonx-com/utils": "^1.0",
    "eonx-com/webhooks": "^0.20",
    "symfony/validator": "^4.3"
  },
  "require-dev": {
    "eonx-com/search": "^2.0",
    "eonx-com/standards": "^0.2",
    "friendsofphp/php-cs-fixer": "^2.9",
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "laravel/lumen-framework": "^6.0",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^8.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "replace": {
    "loyaltycorp/multitenancy": "*"
  },
  "autoload": {
    "psr-4": {
      "LoyaltyCorp\\Multitenancy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\LoyaltyCorp\\Multitenancy\\": "tests/"
    }
  },
  "suggest": {
    "loyaltycorp/search": "Required when using search"
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
